{
  "name": "chat-serverless",
  "version": "1.0.0",
  "description": "chat application with serverless framework",
  "author": "reireias",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon local.js --watch server",
    "build": "NODE_ENV=production nuxt build",
    "postbuild": "webpack",
    "start": "cross-env NODE_ENV=production node .nuxt/dist/local.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "deploy": "NODE_ENV=production sls deploy"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "aws-serverless-express": "^3.3.8",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "express-asyncify": "^1.0.1",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-s3-sync": "^1.14.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpackbar": "^4.0.0"
  }
}
